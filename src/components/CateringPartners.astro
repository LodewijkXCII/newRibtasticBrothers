---
import PartnerImage from "../components/PartnerImage.astro";

const URL =
  "https://admin.ribtasticbrothers.nl/catering-partners?_sort=bedrijf";

const response = await fetch(URL);
const allPartners = await response.json();
---

<section style="overflow: hidden;">
  <h2>Bedrijven die je al voor gingen</h2>
  <div class="cateringPartner">
    {allPartners.map((p) => <PartnerImage partner={p} />)}
  </div>
</section>
